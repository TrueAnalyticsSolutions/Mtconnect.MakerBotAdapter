<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <ApplicationIcon />
    <StartupObject />
    <Company>True Analytics Manufacturing Solutions</Company>
    <Authors>Trais McAllister</Authors>
    <PackageProjectUrl>https://github.com/TrueAnalyticsSolutions/MakerBotAgentAdapterCore</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/TrueAnalyticsSolutions/MakerBotAgentAdapterCore/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/TrueAnalyticsSolutions/MakerBotAgentAdapterCore</RepositoryUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration.Ini" Version="2.2.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="MTConnectAdapterCore">
      <HintPath>..\..\MTConnectAdapterCore\MTConnectAdapterCore\bin\Debug\netcoreapp2.1\publish\MTConnectAdapterCore.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties Resources_4adapter_1json__JSONSchema="http://json.schemastore.org/config" /></VisualStudio></ProjectExtensions>

</Project>
